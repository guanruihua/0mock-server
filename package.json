{
  "name": "rh-dev-mock-server",
  "version": "1.0.3",
  "description": "快速生成一些CRUD的接口, 方便开发使用",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "unpkg": "lib/",
  "jsdelivr": "lib/",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npx rollup -c --extend rollup.config.ts",
    "build2": "rimraf ./lib & tsc",
    "start": "ts-node-dev --respawn --transpile-only ./src/test/run",
    "dev_dao": "ts-node-dev --respawn --transpile-only ./src/test/test_dao",
    "restart": "rimraf dist && npm run build && npm start",
    "eslint": "eslint . --ext .ts --fix",
    "prod": "npm run build && npm run start",
    "pu": "npm publish",
    "pub": "push.bat & rimraf ./dist & tsc & npm publish"
  },
  "keywords": [
    "typescript",
    "express",
    "node",
    "javaScript"
  ],
  "author": "ruihuag",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "canvas": "^2.8.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lorem-ipsum": "^2.0.4",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rh-js-methods": "^0.0.12",
    "rh-mock": "^1.2.0",
    "rh-ts-methods": "^0.0.2",
    "rh-vr-dao": "0.0.5",
    "typescript": "^4.4.4",
    "eslint": "^8.7.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "@types/cors": "^2.8.12",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "rollup": "^2.66.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "@babel/core": "^7.19.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-plugin-prettier": "^4.0.0",
    "node-notifier": "^8.0.2"
  }
}
{
  "name": "rh-dev-mock-server",
  "version": "1.0.9",
  "description": "快速生成一些CRUD的接口, 方便开发使用",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "unpkg": "lib/",
  "jsdelivr": "lib/",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf ./lib & tsc",
    "start": "ts-node-dev --respawn --transpile-only ./test/run",
    "dev_dao": "ts-node-dev --respawn --transpile-only ./src/test/test_dao",
    "restart": "rimraf dist && npm run build && npm start",
    "eslint": "eslint . --ext .ts --fix",
    "prod": "npm run build && npm run start",
    "patch":"npm version patch -f",
    "pub": "npm publish",
    "pub:reload": "push.bat & rimraf ./dist & tsc & npm publish"
  },
  "keywords": [
    "typescript",
    "express",
    "node",
    "javaScript"
  ],
  "author": "ruihuag",
  "license": "ISC",
  "dependencies": {
    "rh-color": "^1.0.2",
    "body-parser": "^1.19.0",
    "canvas": "^2.8.0",
    "cors": "^2.8.5",
    "eslint": "^8.7.0",
    "express": "^4.17.1",
    "lorem-ipsum": "^2.0.4",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rh-js-methods": "^2.1.11",
    "rh-mock": "^1.3.1",
    "rh-ts-methods": "^0.0.2",
    "rh-vr-dao": "0.0.9",
    "typescript": "^4.4.4",
    "ts-node-dev": "^1.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-plugin-prettier": "^4.0.0",
    "node-notifier": "^8.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1"
  }
}
